{
  "buildCommand": null,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth.js?path=$1"
    },
    {
      "source": "/api/content/(.*)",
      "destination": "/api/content.js?path=$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}